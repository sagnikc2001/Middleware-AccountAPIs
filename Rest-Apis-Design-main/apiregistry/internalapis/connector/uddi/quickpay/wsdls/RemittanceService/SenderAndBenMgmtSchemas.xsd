	<simpleType name="YorNType">
		<xsd:annotation>
			<xsd:documentation>
				Valid values are : Y, N
			</xsd:documentation>
		</xsd:annotation>
		<restriction base="string">
			<enumeration value="Y"/>
			<enumeration value="N"/>
		</restriction>
	</simpleType>

	<xsd:simpleType name="correspondentId">
		<xsd:annotation>
			<xsd:documentation>
			</xsd:documentation>
		</xsd:annotation>
		<restriction base="xsd:string">
			<maxLength value="40"/>
			<minLength value="0"/>
		</restriction>
	</xsd:simpleType>
	
	<xsd:simpleType name="TransactionType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="QP"/>
			<xsd:enumeration value="XM"/>
			<xsd:enumeration value="MG"/>
		</xsd:restriction>
	</xsd:simpleType>

</schema>